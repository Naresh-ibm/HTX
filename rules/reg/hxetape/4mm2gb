*---------------------------------------------------------------------------*
*- STANZA 1: Close the re-open the device.                                 -*
*---------------------------------------------------------------------------*
RULE_ID = CLREOP1
OPER = CO

*---------------------------------------------------------------------------*
*- STANZA 2: Rewind to beginning of tape.                                  -*
*---------------------------------------------------------------------------*
RULE_ID = REWIND2
OPER = RW

*---------------------------------------------------------------------------*
*- STANZA 3: Write 32 x 7000 blocks = 224,000 blocks of data.              -*
*---------------------------------------------------------------------------*
RULE_ID = WRITE3
PATTERN_ID = PBEFILE
OPER = W
NUM_OPER = 7000
NUM_BLKS = 32

*---------------------------------------------------------------------------*
*- STANZA 4: Issue a Request Sense Command.                                -*
*---------------------------------------------------------------------------*
RULE_ID = REQSNS4
OPER = RS

*---------------------------------------------------------------------------*
*- STANZA 5: Rewind to beginning of tape.                                  -*
*---------------------------------------------------------------------------*
RULE_ID = REWIND5
OPER = RW

*---------------------------------------------------------------------------*
*- STANZA 6: Read & Compare 32 x 699 = 223,968 blocks of data.             -*
*---------------------------------------------------------------------------*
RULE_ID = READCOM6
PATTERN_ID = PBEFILE
OPER = RC
NUM_OPER = 6999
NUM_BLKS = 32

*---------------------------------------------------------------------------*
*- STANZA 7: Issue a Request Sense Command.                                -*
*---------------------------------------------------------------------------*
RULE_ID = REQSNS7
OPER = RS

*---------------------------------------------------------------------------*
*- STANZA 8: Rewind to beginning of tape.                                  -*
*---------------------------------------------------------------------------*
RULE_ID = REWIND8
OPER = RW

*---------------------------------------------------------------------------*
*- STANZA 9: Write 32 x 350 blocks = 11,200 blocks of data.                -*
*---------------------------------------------------------------------------*
RULE_ID = WRITE9
PATTERN_ID = HXPDB6
OPER = W
NUM_OPER = 350
NUM_BLKS = 32

*---------------------------------------------------------------------------*
*- STANZA 10: Write 2 end-of-file marks.                                   -*
*---------------------------------------------------------------------------*
RULE_ID = WRITFM10
OPER = WEOF
NUM_BLKS = 2

*---------------------------------------------------------------------------*
*- STANZA 11: Write 32 blocks of data.                                     -*
*---------------------------------------------------------------------------*
