*---------------------------------------------------------------------------*
*- STANZA 1: Close and re-open the device.                                 -*
*---------------------------------------------------------------------------*
RULE_ID = CLOREOP1
OPER = CO

*---------------------------------------------------------------------------*
*- STANZA 2: Rewind; to beginning of tape.                                 -*
*---------------------------------------------------------------------------*
RULE_ID = REWIND2
OPER = RW

*---------------------------------------------------------------------------*
*- STANZA 3: Write 32 x 300 blocks = 9600 blocks of data.                  -*
*---------------------------------------------------------------------------*
RULE_ID = WRITE3
PATTERN_ID = HXPDB6
OPER = W
NUM_OPER = 300
NUM_BLKS = 32

*---------------------------------------------------------------------------*
*- STANZA 4: Rewind to beginning of tape.                                  -*
*---------------------------------------------------------------------------*
RULE_ID = REWIND4
OPER = RW

*---------------------------------------------------------------------------*
*- STANZA 5: Write 32 x 300 blocks = 9600 blocks of data.                  -*
*---------------------------------------------------------------------------*
RULE_ID = RDCMP5
PATTERN_ID = HXPDB6
OPER = RC
NUM_OPER = 300
NUM_BLKS = 32

*---------------------------------------------------------------------------*
*- STANZA 6: Rewind to beginning of tape.                                  -*
*---------------------------------------------------------------------------*
RULE_ID = REWIND6
OPER = RW

*---------------------------------------------------------------------------*
*- STANZA 7: Write 32 x 350 blocks = 11,200 blocks of data.                -*
*---------------------------------------------------------------------------*
RULE_ID = WRITE7
PATTERN_ID = HXPDB6
OPER = W
NUM_OPER = 350
NUM_BLKS = 32

*---------------------------------------------------------------------------*
*- STANZA 8: Write 2 end-of-file marks.                                    -*
*---------------------------------------------------------------------------*
RULE_ID = WRITEFM8
OPER = WEOF
NUM_BLKS = 2

*---------------------------------------------------------------------------*
*- STANZA 9: Write 32 blocks.                                              -*
*---------------------------------------------------------------------------*
RULE_ID = WRITE9
OPER = W
NUM_BLKS = 32

*---------------------------------------------------------------------------*
*- STANZA 10: Rewind to beginning of tape.                                 -*
*---------------------------------------------------------------------------*
RULE_ID = REWIND10
OPER = RW

*---------------------------------------------------------------------------*
*- STANZA 11: Skip over 2 file marks.                                      -*
*---------------------------------------------------------------------------*
RULE_ID = SKPFIL11
OPER = SF
NUM_BLKS = 2

*---------------------------------------------------------------------------*
*- STANZA 12: Read 10 blocks of data.                                      -*
*---------------------------------------------------------------------------*
RULE_ID = READ12
OPER = R
NUM_OPER = 1
NUM_BLKS = 10

*---------------------------------------------------------------------------*
*- STANZA 13: Skip backwards 2 file marks.                                 -*
*---------------------------------------------------------------------------*
RULE_ID = SKPFIL13
OPER = SF
NUM_BLKS = -2

*---------------------------------------------------------------------------*
*- STANZA 14: Skip backwards 13 records.                                   -*
*---------------------------------------------------------------------------*
RULE_ID = SKPREC14
OPER = SR
NUM_BLKS = -13

*---------------------------------------------------------------------------*
*- STANZA 15: Skip forward 7 records.                                      -*
*---------------------------------------------------------------------------*
RULE_ID = SKPREC15
OPER = SR
NUM_BLKS = 7

*---------------------------------------------------------------------------*
*- STANZA 16: Read & Compare 2 blocks of data.                             -*
*---------------------------------------------------------------------------*
RULE_ID = RDCMP16
PATTERN_ID = HXPDB6
OPER = RC
NUM_OPER = 1
NUM_BLKS = 2

*---------------------------------------------------------------------------*
*- STANZA 17: Rewind to beginning of tape.                                 -*
*---------------------------------------------------------------------------*
